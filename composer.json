{
  "name": "darkaonline/l5-swagger",
  "description": "Swagger integration to Laravel 5",
  "keywords": [
    "laravel",
    "swagger",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Darius Matulionis",
      "email": "darius@matulionis.lt"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.2.*",
    "zircote/swagger-php": "~2.0"
  },
  "require-dev": {
    "koala-framework/composer-extra-assets": "~2.0",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "L5Swagger\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "extra": {
    "require-bower": {
      "swagger-ui": "2.*"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
